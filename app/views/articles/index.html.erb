<h1>Listing all Articles</h1>

<p><%= link_to "Create new Article", new_article_path%></p>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>
  <% @articles.each do |data|%>
    <tr>
      <td><%= data.title %></td>
      <td><%= data.description %></td>
      <td><%= link_to 'Edit', edit_article_path(data) %></td>
      <td><%= link_to 'Show', article_path(data) %></td>
    </tr>
  <% end %>
</table>
